    "dev:staging": "env-cmd -f .env.staging node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
    "dev:staging:watch": "env-cmd -f .env.staging nodemon src/index.ts",
    "dev:production": "env-cmd -f .env.production node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
    "dev:production:watch": "env-cmd -f .env.production nodemon src/index.ts",
    "deploy:heroku:staged": "sh ./scripts/deployment/heroku/staged.sh",
    "deploy:heroku:production": "sh ./scripts/deployment/heroku/production.sh",
    "start": "node dist/index.js",
    "test": "jest"
